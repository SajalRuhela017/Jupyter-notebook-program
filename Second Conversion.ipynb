    "def sec(x):\n",
    "    ''' Takes time in Seconds and \n",
    "    gives the output in day,hour,minute,second format '''\n",
    "    d=x//86400\n",
    "    h=(x%86400)//3600\n",
    "    m=(x%3600)//60\n",
    "    s=(x%60)\n",
    "    print(f'{d:02}:{h:02}:{m:02}:{s:02}')"
   ]
  },
